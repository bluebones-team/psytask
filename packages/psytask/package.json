{
  "name": "psytask",
  "version": "1.1.0",
  "type": "module",
  "author": "cubxx",
  "description": "JavaScript Framework for Psychology task",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./dist/index.js"
    },
    "./main.css": "./dist/main.css"
  },
  "files": [
    "dist",
    "src",
    "types",
    "index.ts",
    "README.md"
  ],
  "homepage": "https://github.com/bluebones-team/psytask",
  "keywords": [
    "psychology",
    "experiment",
    "task",
    "framework",
    "research",
    "cognitive",
    "behavioral"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluebones-team/psytask.git"
  },
  "bugs": {
    "url": "https://github.com/bluebones-team/psytask/issues"
  },
  "scripts": {
    "dev": "bun index.html --host=0.0.0.0",
    "build": "run-p type-check build-only",
    "build-only": "bun scripts/build.ts",
    "test:e2e": "bun test ./e2e-tests/index.test.ts",
    "type-check": "tsc -b --noEmit"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^18.0.1",
    "@svgdotjs/svg.js": "^3.2.4",
    "puppeteer-core": "^24.13.0"
  }
}
